\n            console.log(ret)\r\n            callback && callback(ret)\r\n            this.index += 20\r\n            if (this.index >= ret.total) {\r\n                this.isFinish = true\r\n            }\r\n        }).fail(()=> {\r\n            console.log('数据异常')\r\n        }).always(()=> {\r\n            this.isLoading = false\r\n            this.wrap.find('.loading').hide()\r\n        })\r\n    }\r\n    //渲染\r\n    render(data) {\r\n        data.subjects.forEach((movie)=> {\r\n            this.$content.append(this.createNode(movie))\r\n        })\r\n    }\r\n}\r\n\r\nexport default Top250page"},"lineCount":null}},"hash":"c6a15c645bccf481e084e2b36d316c8c","cacheData":{"env":{}}}